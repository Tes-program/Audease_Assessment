  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc -p tsconfig.json -w",
    "start": "node dist/index.js",
    "knex:create": "knex migrate:make --knexfile ./knexfile.ts",
    "knex:migrate": "knex migrate:latest --knexfile ./knexfile.ts",
    "knex:rollback": "knex migrate:rollback --knexfile ./knexfile.ts",
    "knex:seed:create": "knex seed:make --knexfile ./knexfile.ts $1",
    "knex:seed:run": "knex seed:run --knexfile ./knexfile.ts $1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
